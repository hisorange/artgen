grammar agl {
  parser eol {
    "\n"
  }

  parser space { " " }
  parser tab { "\t" }
  parser ws {
    ( eol | space | tab )*
  }

  parser identifier {
    [a-zA-Z]+ [a-zA-Z_0-9]*
  }


  lexer grammarName { identifier } -> name
  lexer grammar {
    "grammar" ws grammarName ws ";" ws
  }

  syntax {
    grammar
    parsers?
    tokens?
  }
}